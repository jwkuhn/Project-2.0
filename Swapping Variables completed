// ConsoleApplication10.cpp : This file contains the 'main' function. Program execution begins and ends there.
//

#include "pch.h"
#include <iostream>
#include <stdio.h>
using namespace std;

void swapCharacters(char& a, char& b)
{
	char temp = a;
	a = b;
	b = temp;
}
int main()
{
	char a;
	char b;
	cout << "Before swapping" << endl;
	cout << "Enter a = ";
	cin >> a;

	cout << "Enter b = ";
	cin >> b;

	swapCharacters(a, b);

	cout << "After swapping" << endl;
	cout << "a = " << a << " b = " << b << "\n";

	return 0;
}
